@model IEnumerable<DeBrabander.ViewModels.VATs.VatIndexViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>BTW</h2>

<p>
    @Html.ActionLink("Nieuw BTW tarief ", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VATValue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VATValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VATPercId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VATPercId })
        </td>
    </tr>
}

</table>
